{
    "build": {
      "builder": "DOCKERFILE",
      "dockerfilePath": "./Dockerfile",
      "watchPatterns": [
        "pyproject.toml",
        "src/**",
        "railway.json",
        "Dockerfile"
      ]
    },
    "deploy": {
      "healthcheckPath": "/healthz",
      "healthcheckTimeout": 300,
      "startupTimeout": 300,
      "restartPolicyType": "always",
      "restartPolicyMaxRetries": 10
    }
  }
  